<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_oemedical_consentimientos_tree" model="ir.ui.view">
            <field name="name">view.oemedical.consentimientos.tree</field>
            <field name="model">oemedical.consentimientos</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="nombre"/>
                </tree>
            </field>
        </record>

        <record id="view_oemedical_consentimientos_form" model="ir.ui.view">
            <field name="name">view.oemedical.consentimientos.form</field>
            <field name="model">oemedical.consentimientos</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <field name="nombre" colspan="4"/>
                    <field name="texto" colspan="4"/>
                </form>
            </field>
        </record>


        <record id="view_oemedical_consentimientos_search" model="ir.ui.view">
            <field name="name">view_oemedical_consentimientos_search</field>
            <field name="model">oemedical.consentimientos</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search>
                    <field name="nombre" colspan="4"/>
                </search>
            </field>
        </record>

        <record id="action_oemedical_consentimientos_view" model="ir.actions.act_window">
            <field name="name">Listado de consentimientos</field>
            <field name="res_model">oemedical.consentimientos</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</openerp>